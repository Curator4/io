{
  "name": "io-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot frontend for Io AI assistant",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=../proto --ts_proto_out=src/grpc/generated --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true io.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "ts-proto": "^2.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
